<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
    <Label layoutX="20.0" layoutY="23.0" text="Enter morse code ...">
      <font>
         <Font name="SansSerif Regular" size="13.0" />
      </font>
    </Label>
   <TextField fx:id="input" layoutX="20.0" layoutY="54.0" prefHeight="26.0" prefWidth="483.0">
      <font>
         <Font name="SansSerif Regular" size="13.0" />
      </font>
   </TextField>
   <Label fx:id="lblTwo" layoutX="20.0" layoutY="127.0" text="Your morse translates to ...." visible="false">
      <font>
         <Font name="SansSerif Regular" size="13.0" />
      </font>
   </Label>
   <Label fx:id="lblmorseTranslation" layoutX="20.0" layoutY="165.0">
      <font>
         <Font name="SansSerif Regular" size="13.0" />
      </font>
   </Label>
   <Button fx:id="btnTrans" layoutX="515.0" layoutY="52.0" mnemonicParsing="false" onMouseClicked="#handleTransButton" text="Translate">
      <font>
         <Font name="SansSerif Regular" size="13.0" />
      </font></Button>

</AnchorPane>
